{
  "sbg:id": "json_to_hotspot3d_maf",
  "id": "#json_to_hotspot3d_maf",
  "inputs": [
    {
      "type": ["null", "File"],
      "label": "json",
      "description": "a json created from an annotated vcf",
      "streamable": false,
      "default": "",
      "id": "#json",
      "inputBinding": {
        "position": 0,
        "prefix": "--json=",
        "separate": true,
        "sbg:cmdInclude": true
      },
      "required": false
    }
  ],
  "outputs": [
    {
      "type": ["null", "File"],
      "label": "std_out",
      "description": "standard output",
      "streamable": false,
      "default": "",
      "id": "#std_out",
      "outputBinding": {
        "glob": "\"std.out\"",
        "sbg:metadata": {
          "org": "cgp"
        }
      }
    },
    {
      "type": ["null", "File"],
      "label": "maf",
      "description": "maf file created",
      "streamable": false,
      "default": "",
      "id": "#maf",
      "outputBinding": {
        "glob": {
          "engine": "#cwl-js-engine",
          "script": "\"*.maf\"",
          "class": "Expression"
        },
        "sbg:inheritMetadataFrom": "#normal_bam",
        "sbg:metadata": {
          "from_tool": "json"
        }
      }
    }
  ],
  "requirements": [],
  "hints": [
    {
      "class": "DockerRequirement",
      "dockerPull": "cgrlab/tidyverse:latest"
    },
    {
      "class": "sbg:CPURequirement",
      "value": 1
    },
    {
      "class": "sbg:MemRequirement",
      "value": 1000
    }
  ],
  "label": "json_to_hotspot3d_maf",
  "class": "CommandLineTool",
  "baseCommand": [
    "CPU=`grep -c ^processor /proc/cpuinfo` && ascat.pl -c $CPU -rs Human -ra NCBI37 -pr WGS -pl ILLUMINA -sg ascat_SnpGcCorrections.tsv -q 20 -g L -o ascat_output"
  ],
  "arguments": [
    {
      "position": 99,
      "separate": true,
      "valueFrom": {
        "engine": "#cwl-js-engine",
        "script": "\"&& ls -lR\"",
        "class": "Expression"
      }
    }
  ],
  "stdout": "std.out"
}
